{
    "name": "qlimix/messagebus-scheduler-middleware",
    "type": "library",
    "description": "Schedule messages via the messagebus with this middleware",
    "license": "MIT",
    "keywords": [
        "middleware",
        "messagebus",
        "schedule",
        "scheduler"
    ],
    "authors": [
        {
            "name": "frank",
            "email": "frank@qlimix.net"
        }
    ],
    "require": {
        "php": ">=7.2",
        "qlimix/message-scheduler": "^1.0",
        "qlimix/messagebus": "^1.0",
        "qlimix/serializable": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Qlimix\\MessageBus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qlimix\\Tests\\MessageBus\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "qlimix/code-standard": "^1.0"
    }
}
